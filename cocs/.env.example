# 咸鱼AI客服配置文件示例
# 复制此文件为 .env 并填入实际配置

# 调试模式
DEBUG=false

# 浏览器配置
BROWSER_HEADLESS=false

# AI服务配置
# 选择AI服务类型：dify 或 qwen
AI_SERVICE_TYPE=dify

# Dify配置（当AI_SERVICE_TYPE=dify时使用）
DIFY_API_URL=https://api.dify.ai
DIFY_API_KEY=your_dify_api_key_here

# 阿里云Qwen配置（当AI_SERVICE_TYPE=qwen时使用）
QWEN_API_KEY=your_qwen_api_key_here
QWEN_MODEL_NAME=qwen-turbo

# 置信度阈值（0.0-1.0）
CONFIDENCE_THRESHOLD=0.7

# 微信通知配置
WECHAT_WEBHOOK_URL=https://qyapi.weixin.qq.com/cgi-bin/webhook/send?key=your_webhook_key

# 邮件通知配置（可选）
EMAIL_SMTP_SERVER=smtp.gmail.com
EMAIL_SMTP_PORT=587
EMAIL_USERNAME=your_email@gmail.com
EMAIL_PASSWORD=your_email_password
EMAIL_RECIPIENTS=admin@company.com,support@company.com

# 通知开关
ENABLE_NOTIFICATIONS=true

# 服务器配置
SERVER_HOST=127.0.0.1
SERVER_PORT=8000

# 日志配置
LOG_LEVEL=INFO
LOG_FILE=logs/goofish_ai.log